@model IEnumerable<CRYBZ_CCSB.Models.Vehicle>
@{
    ViewData["LicensePlate"] = "";
}
<h2>Overzicht</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LicencePlate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LicencePlate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LicencePlate">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.LicencePlate">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>

@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}